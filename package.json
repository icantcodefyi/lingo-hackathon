{
  "name": "my-better-t-app",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "elysia": "^1.3.21",
      "@orpc/server": "^1.10.0",
      "@orpc/openapi": "^1.10.0",
      "@orpc/zod": "^1.10.0",
      "better-auth": "^1.3.28",
      "dotenv": "^17.2.2",
      "zod": "^4.1.11",
      "typescript": "^5.8.2",
      "tsdown": "^0.15.5",
      "@orpc/client": "^1.10.0"
    }
  },
  "scripts": {
    "check": "biome check --write .",
    "dev": "bun run --filter '*' dev",
    "build": "bun run --filter '*' build",
    "check-types": "bun run --filter '*' check-types",
    "dev:native": "bun run --filter native dev",
    "dev:web": "bun run --filter web dev",
    "dev:server": "bun run --filter server dev",
    "db:push": "bun run --filter @my-better-t-app/db db:push",
    "db:studio": "bun run --filter @my-better-t-app/db db:studio",
    "db:generate": "bun run --filter @my-better-t-app/db db:generate",
    "db:migrate": "bun run --filter @my-better-t-app/db db:migrate"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.44",
    "@ai-sdk/openai": "^2.0.67",
    "ai": "^5.0.93",
    "dotenv": "^17.2.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "tsdown": "^0.15.5",
    "@types/bun": "^1.2.6"
  },
  "packageManager": "bun@1.2.17"
}
